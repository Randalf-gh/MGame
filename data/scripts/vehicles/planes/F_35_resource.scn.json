[
  {
    "declare_macro": "load_F_35_part",
    "content": {
      "call": "obj_resource",
      "literals": {
        "name": "%NAME",
        "filename": "models/sketchfab.com/f-35c-lightning-ii/$SUBDIR/$NAME.obj",
        "position": [0, -3, 0],
        "rotation": [0, 180, 0],
        "scale": [10, 10, 10],
        "center_distances": ["%DIST_MIN", "%DIST_MAX"],
        "blend_mode": "continuous",
        "alpha_distances": [0, 0, "inf", "inf"],
        "cull_faces_alpha": false,
        "occluded_pass": "none",
        "occluder_pass": "%OCCLUDER_PASS",
        "aggregate_mode": "none",
        "transformation_mode": "all",
        "triangle_tangent_error_behavior": "zero",
        "werror": false
      }
    }
  },
  {
    "declare_macro": "load_F_35_hitbox_part",
    "content": [
      {
        "playback": "load_F_35_part",
        "literals": {
          "NAME": "%NAME",
          "SUBDIR": "hitbox",
          "DIST_MIN": 0,
          "DIST_MAX": "inf",
          "OCCLUDER_PASS": "none"
        }
      },
      {"call": "gen_triangle_rays", "literals": {"name": "%NAME", "npoints": 5, "lengths": [0.7, 0.7, 0.7], "delete_triangles": false}},
      {"call": "modify_physics_material_tags", "literals": {"resource_name": "%NAME", "add": "obj_chassis|attr_convex"}}  
    ]
  },
  {"playback": "load_F_35_hitbox_part", "literals": {"NAME": "F_35_hitbox_front"}},
  {"playback": "load_F_35_hitbox_part", "literals": {"NAME": "F_35_hitbox_mid"}},
  {"playback": "load_F_35_hitbox_part", "literals": {"NAME": "F_35_hitbox_rear"}},
  {"playback": "load_F_35_hitbox_part", "literals": {"NAME": "F_35_hitbox_rudder_left"}},
  {"playback": "load_F_35_hitbox_part", "literals": {"NAME": "F_35_hitbox_rudder_right"}},
  {
    "call": "obj_resource",
    "literals": {
      "name": "F_35_tirelines",
      "filename": "models/primitives/empty.obj",
      "position": [0, 0, 0],
      "rotation": [0, 0, 0],
      "scale": [1, 1, 1],
      "center_distances": [0, 501],
      "blend_mode": "off",
      "alpha_distances": [0, 0, "inf", "inf"],
      "occluded_pass": "none",
      "occluder_pass": "none",
      "aggregate_mode": "none",
      "transformation_mode": "all"
    }
  },
  {"call": "modify_physics_material_tags", "literals": {"resource_name": "F_35_tirelines", "add": "obj_tire_line"}},
  {"call": "gen_ray", "literals": {"name": "F_35_tirelines", "from": [-0.7, -0.2, -1.35], "to": [-0.7, -1, -1.35]}}, {"comment": "left front"},
  {"call": "gen_ray", "literals": {"name": "F_35_tirelines", "from": [ 0.7, -0.2, -1.35], "to": [ 0.7, -1, -1.35]}}, {"comment": "right front"},
  {"call": "gen_ray", "literals": {"name": "F_35_tirelines", "from": [-0.7, -0.2,  1.20], "to": [-0.7, -1,  1.20]}}, {"comment": "left rear"},
  {"call": "gen_ray", "literals": {"name": "F_35_tirelines", "from": [ 0.7, -0.2,  1.20], "to": [ 0.7, -1,  1.20]}}, {"comment": "right rear"},
  {
    "declare_macro": "load_F_35",
    "content": {
      "playback": "load_F_35_part",
      "literals": {
        "NAME": "F_35$DECIMATE",
        "SUBDIR": "obj",
        "DIST_MIN": "%DIST_MIN",
        "DIST_MAX": "%DIST_MAX",
        "OCCLUDER_PASS": "%OCCLUDER_PASS"
      }
    }
  },
  {"playback": "load_F_35", "literals": {"DIST_MIN": 0  , "DIST_MAX": 100 , "OCCLUDER_PASS": "none",               "DECIMATE":""}},
  {"playback": "load_F_35", "literals": {"DIST_MIN": 0  , "DIST_MAX": 100 , "OCCLUDER_PASS": "none",               "DECIMATE":"_d3"}},
  {"playback": "load_F_35", "literals": {"DIST_MIN": 100, "DIST_MAX": 1000, "OCCLUDER_PASS": "lightmap_black_local_instances", "DECIMATE":"_d1"}}
]
