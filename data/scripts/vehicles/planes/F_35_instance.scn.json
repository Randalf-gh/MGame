          "wheel": "wheel_left_front_node$SUFFIX",
          "vehicle_mount_0": [-0.7, -0.45, -1.35],
          "vehicle_mount_1": [-0.7, 0, -1.35],
          "radius": 0.3,
          "engine": "wheels",
          "brake_force": 50000,
          "brake_torque": 400,
