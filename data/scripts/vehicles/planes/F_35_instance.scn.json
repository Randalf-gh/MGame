      {"call": "set_inventory_capacity", "arguments": {"inventory_node": "car_node$SUFFIX", "item_type": "F_35_ammo", "capacity": 40}},
      {"call": "add_to_inventory", "arguments": {"inventory_node": "car_node$SUFFIX", "item_type": "F_35_ammo", "amount": 20}},
      {
        "call": "gun",
        "arguments": {
          "node": "main_gun_end_node$SUFFIX",
          "parent_rigid_body_node": "car_node$SUFFIX",
          "punch_angle_node": null,
          "cool_down": 0.5,
          "ammo_type": "F_35_ammo",
          "bullet_type": "F_35_bullet",
          "punch_angle_idle_std": 0,
          "punch_angle_shoot_std": 0
        }
      },
      {
        "comment": [
          "28000 pounds = 124550 Newton",
          "43000 pounds = 191274 Newton"
        ]
      },
      {
