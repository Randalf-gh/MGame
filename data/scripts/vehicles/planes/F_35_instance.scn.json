          "angle_yz": 1,
          "angle_zz": 1,
          "drag": [2, 8, 2],
          "wing_id": 3
        }
      },
      {"comment": "4: yaw left"},
      {
        "call": "wing",
        "arguments": {
          "vehicle": "car_node$SUFFIX",
          "angle_of_attack_node": "left_rudder_node$SUFFIX",
          "position": [-0.5, 0.5, 6],
          "rotation": [0, -20, 110],
          "fac_v": [0, 300, 600, 850],
          "fac_c": [1, 1, 0.25, 0.125],
          "lift_c": 0,
          "angle_yz": 0.2,
          "angle_zz": 0.2,
          "drag": [0, 3, 0],
          "wing_id": 4
        }
      },
      {"comment": "5: yaw right"},
      {
        "call": "wing",
        "arguments": {
          "vehicle": "car_node$SUFFIX",
          "angle_of_attack_node": "right_rudder_node$SUFFIX",
          "position": [0.5, 0.5, 6],
          "rotation": [0, 20, -110],
          "fac_v": [0, 300, 600, 850],
          "fac_c": [1, 1, 0.25, 0.125],
