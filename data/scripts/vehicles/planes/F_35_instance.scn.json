      },
      {"comment": "3: rear right (roll + pitch)"},
      {
        "call": "wing",
        "arguments": {
          "vehicle": "car_node$SUFFIX",
          "angle_of_attack_node": "right_rear_wing_node$SUFFIX",
          "position": [1, -0.2, 6],
          "rotation": [0, 0, 0],
          "fac_v": [0, 300, 600, 850],
          "fac_c": [1, 1, 0.25, 0.125],
