          "vehicle": "car_node$SUFFIX",
          "wheel": "wheel_right_front_node$SUFFIX",
          "vehicle_mount_0": [ 0.7, -0.45, -1.35],
          "vehicle_mount_1": [ 0.7, 0, -1.35],
          "radius": 0.3,
          "engine": "wheels",
          "brake_force": 50000,
          "brake_torque": 400,
          "Ks": 1e5,
          "Ka": 4e3,
          "musF": [4e3, 8e3],
          "musC": [1.1, 0.97],
