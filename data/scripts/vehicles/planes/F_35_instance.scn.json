      {"comment": "1: front right (roll + pitch)"},
      {
        "call": "wing",
        "arguments": {
          "vehicle": "car_node$SUFFIX",
          "angle_of_attack_node": "right_front_aileron_node$SUFFIX",
          "brake_angle_node": "right_flap_node$SUFFIX",
          "position": [4, -0.2, -0.2],
          "rotation": [0, 0, 0],
          "fac_v": [0, 300, 600, 850],
          "fac_c": [1, 1, 0.25, 0.125],
          "lift_c": 25,
          "angle_yz": 1,
          "angle_zz": 1,
          "drag": [7, 60, 7],
          "wing_id": 1,
          "trail_source": {
